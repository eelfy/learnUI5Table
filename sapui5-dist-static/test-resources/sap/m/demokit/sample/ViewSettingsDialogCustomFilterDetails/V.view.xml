<mvc:View
		controllerName="sap.m.sample.ViewSettingsDialogCustomFilterDetails.C"
		xmlns:l="sap.ui.layout"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns="sap.m">
	<l:VerticalLayout
			class="sapUiContentPadding"
			width="100%">
		<Button
				text="Search Contains"
				press="handleOpenDialogSearchContains"
				ariaHasPopup="Dialog" />
		<Button
				text="Search Any Word Starts With"
				press="handleOpenDialogSearchWordsStartWith"
				ariaHasPopup="Dialog" />
		<Button
				text="Search Case Sensitive Contains"
				press="handleOpenDialogCustomSearch"
				ariaHasPopup="Dialog" />
	</l:VerticalLayout>
</mvc:View>