{
  "_version": "1.12.0",
  "sap.app": {
	"id": "sap.ui.comp.sample.smartfilterbar_dialog",
	"type": "application",
	"applicationVersion": {
	  "version": "1.0.0"
	},
	"ach": "CA-UI5-COMP"
  },
  "sap.ui": {
	"technology": "UI5"
  },
  "sap.ui5": {
    "flexEnabled": true,
	"rootView": {
	  "id": "IDView",
	  "viewName": "sap.ui.comp.sample.smartfilterbar_dialog.SmartFilterBar",
	  "type": "XML",
	  "height": "100%",
	  "async": true
	},
	"handleValidation": true,
	"dependencies": {
	   "minUI5Version": "1.88.0",
				"libs": {
					"sap.m": {
						"lazy": true
					},
					"sap.ui.layout": {
						"lazy": true
					},"sap.ui.fl": {
						"lazy": true
					},
					"sap.ui.mdc": {
						"lazy": true
					},
					"sap.ui.rta": {
						"lazy": true
					},
					"sap.ui.dt": {
						"lazy": true
					}
				}
	   },
	  "models": {
			"": {
				"dataSource": "main",
				"preload": true,
				"settings": {
					"autoExpandSelect": true,
					"operationMode": "Server",
					"synchronizationMode": "None"
				},
				"type": "sap.ui.model.odata.v2.ODataModel"
				},
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            }
      }
   }
}
