<mvc:View
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:smartchart="sap.ui.comp.smartchart"
	xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:sl="sap.ui.comp.navpopover"
	xmlns:data="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	xmlns="sap.m"
	controllerName="sap.ui.comp.sample.smartchart.calendarYearQuarter.SmartChart"
	height="100%">
	<VBox fitContainer="true">
		<smartchart:SmartChart
			id ="smartChartCalendarYearQuarter"
			entitySet="SEPMRA_C_PD_ProductSalesData"
			enableAutoBinding="true"
			useVariantManagement="true"
			persistencyKey="PKeyChartExampleCalendarYearQuarter"
			useChartPersonalisation="true"
			showFullScreenButton="true"
			selectionMode="Multi"
			showChartTooltip="true"
			showDrillBreadcrumbs="false"
			showDetailsButton="false"
			showDrillButtons="false"
			showSemanticNavigationButton="false"
			activateTimeSeries="true"
			height="100%">
		</smartchart:SmartChart>
	</VBox>
</mvc:View>
