<mvc:View
	xmlns="sap.m"
	xmlns:layout="sap.ui.layout"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:es="sap.ushell.renderers.fiori2.search.inputhelp"
	controllerName="sap.ushell.shells.demo.searchRoutingApp.controller.View2">
	<Page
		title="Search Selection Page"
		showNavButton="true"
		navButtonPress="onToPage1">
		<content>
			<es:SearchInputHelpWizard id="searchInputHelpWizard"/>
		</content>
		<footer>
			<Toolbar>
				<ToolbarSpacer/>
				<Button icon="sap-icon://arrow-right" type="Emphasized" press="onToPage3" tooltip="Select Items to next Page"/>
			</Toolbar>
		</footer>
	</Page>
</mvc:View>
