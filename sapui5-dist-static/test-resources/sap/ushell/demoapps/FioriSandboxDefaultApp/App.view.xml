<core:View
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    controllerName="sap.ushell.demo.FioriSandboxDefaultApp.App">
    <App>
        <Page title="Default Application for the Fiori Launchpad Sandbox">
            <content>
                <Panel headerText="Apps Configured for Sandbox Shell">
                    <List id="applist" noDataText="No applications found in configuration" items="{/apps}">
                        <CustomListItem>
                            <Link href="{href}" text="{appDescription}" />
                        </CustomListItem>
                    </List>
                </Panel>
            </content>
        </Page>
    </App>
</core:View>
