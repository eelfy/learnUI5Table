<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:w="sap.ui.integration.widgets"
	displayBlock="true">
	<Page title="{i18n>appTitle}">
		<subHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<Title text="Calendar Card"/>
				<ToolbarSpacer/>
			</Toolbar>
		</subHeader>

		<Panel>
			<w:Card id="calendarCard" class="sapUiMediumMargin" manifest="{manifests>/calendarCard/calendarCard}" width="700px" height="auto" />
		</Panel>
	</Page>
</mvc:View>